{
  "mcpServers": {
    "sequential-thinking": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "nx-mcp": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx",
        "-y",
        "nx-mcp@latest",
        "D://projects/angular-3d-workspace"
      ]
    },
    "angular-cli": {
      "command": "cmd.exe",
      "args": ["/c", "npx", "-y", "@angular/cli", "mcp"]
    },
    "chrome-devtools": {
      "command": "cmd.exe",
      "args": ["/c", "npx", "-y", "chrome-devtools-mcp@latest"]
    }
  }
}
